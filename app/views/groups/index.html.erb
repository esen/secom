<div id="groups"></div>

<script type="text/javascript">
    window.onload = function () {
        // Secom is the app name
        window.router = new Secom.Routers.GroupsRouter({
            groups: <%= @groups.to_json.html_safe -%>,
            levels: <%= @levels.to_json.html_safe -%>
        });
        Backbone.history.start();
    };
</script>