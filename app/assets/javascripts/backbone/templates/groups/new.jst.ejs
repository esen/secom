<div class="page-header">
  <h1>Жаңы группа</h1>
</div>


<form id="new-group" name="group" class='form-horizontal'>
    <div class="control-group">
        <label for="name" class="control-label"> Аталышы:</label>
            <div class="controls">
                <input type="text" class="text_field" name="name" id="name" value="<%= name %>" >
            </div>
    </div>

    <div class="control-group">
        <label for="level_id" class="control-label"> Даражасы:</label>
            <div class="controls">
                <select name="level_id" id="level_id">
                </select>
            </div>
    </div>

    <div class="control-group">
        <label for="price" class="control-label"> Наркы:</label>
            <div class="controls">
                <input type="text" class="text_field" name="price" id="price" value="<%= price %>" >
            </div>
    </div>

    <div class="control-group">
        <label for="started_at" class="control-label"> Башталышы:</label>
            <div class="controls">
                <input type="text" class="text_field" name="started_at" id="started_at" value="<%= started_at %>" >
            </div>
    </div>

    <div class="form-actions">
        <a href="#/index" class="btn btn-small">Артка</a>
        <input type="submit" value="Группаны ач" class="btn btn-small btn-primary"/>
    </div>

</form>