<div class="page-header">
  <h1>Edit level</h1>
</div>


<form id="edit-level" name="level" class='form-horizontal'>
    <div class="control-group">
        <label for="name" class="control-label"> name:</label>
            <div class="controls">
                <input type="text" class="text_field" name="name" id="name" value="<%= name %>" >
            </div>
    </div>

    <div class="form-actions">
        <a href="#/index" class="btn btn-small">Cancel</a>
        <input type="submit" value="Update Level" class="btn btn-small btn-primary"/>
    </div>

</form>